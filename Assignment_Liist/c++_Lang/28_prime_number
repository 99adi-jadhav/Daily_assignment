/*
  Date : 3 october 2024
  Write a c++ program Check given number is prime number 
  Expected Output :    Number is : 19
  	                   Given Number is Prime Number
*/
#include <iostream>
using namespace std;
int main() 
{
    int num = 19;
    cout << "Number is :" << num << endl;
    
    if(num % 2 == 1)
    {
	cout << "Given Number is Prime Number" << endl;
    } 
    else
    {
	cout << "Given Number is not  Prime Number" << endl;
    }
    return 0;
}



/*
	Output :
			               Number is : 19
  	                 Given Number is Prime Number
*/
