/*
 Date : 22 sept 2024
 Write a c++ program to print factorial Number 
 Expected output : Factorial of 5 = 120
*/


#include <iostream>

using namespace std;

int main() 
{
    int num, factorial = 1;

    cout << "Enter a number: ";
    cin >> num;

    for (int i = 1; i <= num; i++)
     {
        factorial *= i;
     }

    cout << "Factorial of " << num << " = " << factorial << endl;
    return Â  
 0;
}


/*
	output :  Factorial of 5 = 120

*/
