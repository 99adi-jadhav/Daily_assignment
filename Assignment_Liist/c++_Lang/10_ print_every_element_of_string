/*
  Date : 16 sept 2024
  Write a c++ program to print every element of string 
  Expected output : Enter String: Bharat 
                          				B 
                          				h 
                          				a 
                          				r 
                          				a 
                          				t 
  			
*/
#include <iostream>
#include <cstring>

using namespace std;

int main() 
{
    char sentence[100];

    cout << "Enter String: ";
    cin >> sentence; 

    for (int i = 0; i < strlen(sentence); i++) 
    {
        cout << sentence[i] << endl; 
    }

    return 0;
}


/*
	output : Enter String: Bharat 
                  				B 
                  				h 
                  				a 
                  				r 
                  				a 
                  				t 
*/
